<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{% block nav %}
			<ul class="navbar justify-content-center bg-dark">
				<li class="nav-item">
					<a class="nav-link active"  href="{{path('index')}}">Home</a>
				</li>
				<li class="nav-item">
					<a class="nav-link" href="{{ path('account_index') }}">account</a>
				</li>
				<li class="nav-item">
					<a class="nav-link" href="{{ path('owner_index') }}">Owner</a>
				</li>
                <li class="nav-item">
					<a class="nav-link" href="{{ path('app_register') }}">Registration</a>
				</li>
                <li class="nav-item">
					<a class="nav-link" href="{{ path('login') }}">Login</a>
				</li>
			</ul>
		{% endblock %}
		{# Run `composer require symfony/webpack-encore-bundle`
		           and uncomment the following Encore helpers to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		{% block body %}{% endblock %}
	</body>
</html>
